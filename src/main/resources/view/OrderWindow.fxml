<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Stage?>
<?import javafx.scene.layout.StackPane?>

<fx:root title="Orders" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/help_icon.png" />
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@DarkTheme.css" />
            </stylesheets>

            <VBox fx:id="personList" styleClass="pane-with-border" minWidth="540" prefWidth="540" VBox.vgrow="ALWAYS">
                <padding>
                    <Insets top="5" bottom="5" left="5" />
                </padding>
                <StackPane fx:id="orderListPanelPlaceholder" VBox.vgrow="ALWAYS"/>
                <StackPane fx:id="orderListView" VBox.vgrow="ALWAYS"/>
            </VBox>
        </Scene>
    </scene>
</fx:root>
